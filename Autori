import java.util.*;
import java.lang.*;
import java.io.*;

public class Main
{
	public static void main (String[] args) throws java.lang.Exception
	{
		//your code here
                Scanner sc=new Scanner (System.in);
                String str=sc.next();
                String s=str.toUpperCase();
                String ret[]=s.split("-");
                
                for(int i=0; i<ret.length; i++)
                        {
                                String word=ret[i];
                                char ch=word.charAt(0);
                                System.out.print(ch);
                        }
	}
}
